{
  "version": 2,
  "alias": ["jji.immo"],
  "builds": [
    {
      "src": "README.md",
      "use": "@now/md",
      "config": {
        "title": "Jim's Slides",
        "language": "en",
        "css": "theme/markdown.css"
      }
    },
    {
      "src": "**/*.mdx",
      "use": "@now/mdx-deck"
    },
    {
      "src": "**/*.css",
      "use": "@now/static"
    }
  ],
  "regions": ["bru"],
  "routes": [{ "src": "/", "dest": "/README.html" }]
}
